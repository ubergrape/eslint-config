{
  "name": "eslint-config-ubergrape",
  "description": "Grape's ESLint configuration",
  "version": "5.4.0",
  "author": {
    "name": "Oleg Slobodskoi",
    "email": "oleg008@gmail.com"
  },
  "keywords": [
    "eslint",
    "config",
    "react",
    "jsx",
    "linter",
    "ubergrape"
  ],
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ubergrape/eslint-config.git"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "babel-eslint": ">=8.2.3",
    "eslint": ">=4.19.1",
    "eslint-config-airbnb": ">=16.1.0",
    "eslint-config-prettier": ">=2.9.0",
    "eslint-plugin-import": ">=2.2.0",
    "eslint-plugin-jsx-a11y": ">=6.0.3",
    "eslint-plugin-prettier": ">=2.6.0",
    "eslint-plugin-react": ">=7.9.1",
    "prettier": ">=1.13.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
